{
  "name": "gitter-clone",
  "version": "0.0.1",
  "description": "Gitter Clone",
  "main": "./compiled/server/",
  "engines": {
    "node": "7.6.0",
    "npm": "4.1.2"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./compiled/server",
    "dev": "NODE_ENV=development nodemon ./src/server/",
    "test": "jest --silent --coverage --runInBand --forceExit",
    "lint": "eslint --ext .jsx --ext .js .",
    "build": "rimraf ./compiled && npm run build:server && npm run build:client && sw-precache --config=./config/sw-precache.js",
    "build:client": "NODE_ENV=production webpack --progress --profile --colors",
    "build:server": "npm run babel -- ./src/ -d ./compiled",
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,static,client,*.spec.js"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "jest",
    "enzyme",
    "socket.io"
  ],
  "author": "Tyler Crosse",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-winston": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "mongoose": "^4.7.7",
    "node-sass": "^4.5.3",
    "normalizr": "^3.1.0",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "qs": "^6.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-modal": "^1.6.5",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "react-scroll": "^1.4.5",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.2",
    "redux-persist": "^4.8.2",
    "redux-socket.io": "^1.3.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "sequelize": "^4.1.0",
    "shortid": "^2.2.8",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^2.1.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.3.3",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux-devtools-extension": "^2.13.0",
    "redux-mock-store": "^1.2.3",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "sw-precache": "^5.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^3.0.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "lcov"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/config/styleMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/config/",
      "/static/",
      "/compiled/",
      "/coverage/",
      "/webpack*",
      "/ecosystem*",
      "/*store/",
      "/models/",
      "/migrations/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 67,
        "functions": 67,
        "lines": 77
      }
    },
    "coverageReporters": [
      "text-summary",
      "html",
      "lcov"
    ]
  }
}
